{
  "family": "grpchat-http-service",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "grpchat-http-service",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "grpchat-grpc-service",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "image": "413025517373.dkr.ecr.us-east-1.amazonaws.com/grpchat-http:f0ab9e9",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::413025517373:role/GrpChatGRPCEcsExecutionRole"
}

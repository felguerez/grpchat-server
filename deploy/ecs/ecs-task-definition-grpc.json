{
  "family": "grpchat-grpc-service",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::413025517373:role/GrpChatGRPCEcsExecutionRole",
  "containerDefinitions": [
    {
      "name": "grpchat-grpc-container",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "grpchat-grpc-service",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "image": "413025517373.dkr.ecr.us-east-1.amazonaws.com/grpchat-grpc:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 50051
        }
      ],
      "environment": [
        {
          "name": "SPOTIFY_CLIENT_ID",
          "value": "b5d80c20bbdc45ccb1138723e6099aaf"
        },
        {
          "name": "SPOTIFY_CLIENT_SECRET",
          "value": "e8918949cbbe4f009a95bbd1d912f8d2"
        },
        {
          "name": "SPOTIFY_REDIRECT_CALLBACK_URL",
          "value": "http://localhost:8080/callback"
        },
        {
          "name": "CHAT_MESSAGES_TABLE",
          "value": "ChatMessages"
        },
        {
          "name": "SPOTIFY_ACCESS_TOKENS_TABLE",
          "value": "SpotifyAccessTokens"
        }
      ]
    }
  ]
}
